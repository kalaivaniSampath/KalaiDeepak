<apex:page standardController="Account" recordSetVar="acc" sidebar="false">
    <apex:form >
        <apex:pageblock >
            <apex:pageblockTable value="{!acc}" var="a">
            
            <apex:column headerValue="Account Name" value="{!a.Name}"/>
            <apex:column headerValue="Account Id" value="{!a.Id}"/>
            <apex:column headerValue="Type"  value="{!a.Type}"/>
            
            
            </apex:pageblockTable>
        </apex:pageblock>      
    </apex:form>
   
</apex:page>