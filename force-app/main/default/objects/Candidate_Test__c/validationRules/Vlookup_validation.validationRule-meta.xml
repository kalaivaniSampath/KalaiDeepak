<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Vlookup_validation</fullName>
    <active>false</active>
    <errorConditionFormula>IF( Name ==VLOOKUP( $ObjectType.Candidate_Test__c.Fields.Name ,  $ObjectType.Candidate_Test__c.Fields.Name ,    Name ), true, false)</errorConditionFormula>
    <errorDisplayField>Name</errorDisplayField>
    <errorMessage>Same Name already exist.</errorMessage>
</ValidationRule>
