<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Fist_name_containbegintext_email_format</fullName>
    <active>true</active>
    <description>AND( BEGINS( FirstName ,&quot;test&quot;), CONTAINS(Email,&quot; test@test.com&quot;) )</description>
    <errorConditionFormula>IF(Email == &quot;test@test.com&quot;,true, false)</errorConditionFormula>
    <errorDisplayField>Email</errorDisplayField>
    <errorMessage>please verify  email should be different format</errorMessage>
</ValidationRule>
